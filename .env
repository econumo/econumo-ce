###> doctrine/doctrine-bundle ###
# Database driver: sqlite or postgresql
DATABASE_DRIVER=sqlite

# SQLite configuration
DATABASE_NAME=db
SQLITE_DATABASE_URL=sqlite:///%kernel.project_dir%/var/db/${DATABASE_NAME}.sqlite

# PostgreSQL configuration
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=econumo
POSTGRES_USER=econumo
POSTGRES_PASSWORD=econumo
POSTGRES_DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_DB}?serverVersion=17&charset=utf8

# Active database URL (automatically set based on DATABASE_DRIVER)
# Use SQLITE_DATABASE_URL for sqlite, POSTGRES_DATABASE_URL for postgresql
DATABASE_URL=${SQLITE_DATABASE_URL}
###< doctrine/doctrine-bundle ###

